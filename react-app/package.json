{
  "name": "shopmetrics-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@nivo/core": "^0.87.0",
    "@nivo/pie": "^0.87.0",
    "@nivo/sankey": "^0.87.0",
    "@tanstack/react-table": "^8.17.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@wordpress/i18n": "^5.25.0",
    "date-fns": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-scripts": "5.0.1",
    "recharts": "^2.15.3",
    "rsuite": "^5.80.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "extract-strings": "node extract-strings.js",
    "generate-translations": "node extract-strings.js --post-build",
    "clean-pot-duplicates": "node clean-pot-duplicates.js",
    "build": "npm run extract-strings && react-scripts build && rimraf ../admin/js/dist && copyfiles -f \"build/static/js/*.js\" \"build/static/js/*.js.map\" \"build/static/css/*.css\" \"build/static/css/*.css.map\" ../admin/js/dist && npm run generate-translations",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "rimraf": "^6.0.1"
  }
}
